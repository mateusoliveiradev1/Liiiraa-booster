{
  "name": "liiiraa-booster",
  "version": "0.1.0",
  "description": "Liiiraa Booster desktop app built with Electron and Vite",
  "main": "src/main/index.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "start": "electron .",
    "build": "vite build",
    "dist": "electron-builder",
    "pretest": "node scripts/pretest.js",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "i18next": "^25.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.0-beta.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "tailwindcss": "^3.4.0",
    "vite": "^5.2.0",
    "electron": "^28.0.0"
  },
  "build": {
    "appId": "com.liiiraa.booster",
    "productName": "Liiiraa Booster",
    "files": [
      "dist/**/*",
      "src/main/**/*",
      "scripts/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
